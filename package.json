{
  "name": "@tenum/chakra-ui-library",
  "version": "1.1.0",
  "private": false,
  "type": "module",
  "description": "Tenum Chakra UI Web Components UI Library - Module Federation compatible",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "start": "webpack serve --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "pretest": "npm run build",
    "test": "jest --passWithNoTests",
    "test:only": "jest --passWithNoTests",
    "check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@chakra-ui/react": "2.10.9",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@r2wc/react-to-web-component": "2.0.4",
    "@rjsf/chakra-ui": "5.24.13",
    "@rjsf/validator-ajv8": "5.24.13",
    "tenum-wc-registration": "git+https://github.com/TENUM-Dev/tenum-wc-registration.git",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "18.3.24"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  }
}
